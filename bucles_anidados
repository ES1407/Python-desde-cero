num_list = [33,42,5,66,77,22,16,79,36,62,78,43,88,39,53,67,89,11]

count = 0 

for index, value in enumerate(num_list):
    count += 1
    print(index, value)
    if value > 45:
        print("Mas de 45:", value)
        break

    elif value == 36:
        print("Numero encontrado en la posicion: ", index)   
        
    else:
        print("Menos de 45:", value)

print(count)




        
   
    